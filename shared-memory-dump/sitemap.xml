<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"> 
  <url>
    <loc>{{blogURL}}</loc> 
  </url>
  <url>
    <loc>{{blogURL}}/archives/</loc> 
    <changefreq>daily</changefreq> 
  </url>
  {{#menu}}  
  <url>
    <loc>{{blogURL}}{{url}}/</loc>
    <lastmod>{{#formatDate}}YYYY-MM-DD{{/formatDate}}</lastmod> 
    <changefreq>daily</changefreq>
  </url>
  {{/menu}}    
  {{#allEntries}}  
  <url>
    <loc>{{blogURL}}{{url}}/</loc> 
    <lastmod>{{#formatDate}}YYYY-MM-DD{{/formatDate}}</lastmod> 
  </url>
  {{/allEntries}}  
</urlset>